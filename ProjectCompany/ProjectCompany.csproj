    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>6d6757d4-75a8-4ba7-a019-b0f160288420</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="8.0.6" />
